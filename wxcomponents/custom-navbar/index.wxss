.custom-navbar {
  --height: 44px;
  overflow: hidden;
}

.custom-navbar.android {
  --height: 48px;
}

.navbar-placeholder {
  height: var(--height);
}

.navbar-placeholder.android {
  height: 48px;
}

.navbar-inner {
  z-index: 5001;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: var(--height);
  display: flex;
  align-items: center
}

.navbar-border {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 1px;
    transform: scaleY(50%);
    background-color: #d8d8d8;
}

.navbar-left {
  display: flex;
  align-items: center;
  padding-left: 8px;
}

.weui-navigation-bar__right {
  margin-right: 16px
}

.navbar-left {
  position: relative;
}

.weui-active {
  opacity: .5
}

.btn-goback-wrapper {
  padding: 0 10px;
}

.btn-goback:active {
  opacity: .5
}

